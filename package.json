{
  "name": "lwh_react",
  "version": "0.1.4",
  "main": "lib/index.js",
  "private": false,
  "files": [
    "lib"
  ],
  "scripts": {
    "publish:build": "rm -rf lib && npm run build",
    "dev": "webpack-dev-server --progress --colors --profile --config build/webpack.config.dev.js ",
    "build:pro": "rm -rf docs/dist && webpack --progress --colors --profile --config build/webpack.config.prod.js --profile --json > stats.json",
    "server": "npm run build:pro && node docs/server/server.run.js",
    "add:package": "node build/package.create.js",
    "build:other-entry": "node build/publish_build/build_other_entry.js",
    "build:component-entry": "node build/publish_build/create_components_entry.js > build/publish_build/components_entry.json",
    "build:webpack": "webpack --config build/publish_build/webpack.base.conf.js --hide-modules",
    "build": "npm run build:other-entry && npm run build:component-entry && npm run build:webpack",
    "server:dev": "pm2-dev start ecosystem.config.js",
    "server:setup": "pm2 deploy ecosystem.config.js production setup",
    "server:publish:first": "npm run server:setup && pm2 deploy ecosystem.config.js production",
    "server:publish": "pm2 deploy ecosystem.config.js production",
    "see": "webpack-bundle-analyzer stats.json -p 8888",
    "predeploy": "npm run build:pro",
    "deploy": "gh-pages -d ./docs/dist"
  },
  "description": "react 常用业务组件库",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hui0830/lwh_react.git"
  },
  "keywords": [
    "React、组件库"
  ],
  "author": "1285227393@qq.com、liwenhui",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hui0830/lwh_react/issues"
  },
  "homepage": "https://Hui0830.github.io/lwh_react",
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "autoprefixer": "^9.4.9",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.14.1",
    "fs-extra": "^7.0.1",
    "happypack": "^5.0.1",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "koa": "^2.7.0",
    "koa-static": "^5.0.0",
    "koa-static-cache": "^5.1.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-loadable": "^5.5.0",
    "react-markdown-loader": "^1.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.3.1",
    "replace-in-file": "^3.4.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prop-types": "^15.7.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  }
}
